@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Roboto:300,400,500,700" type="text/css">
    @Scripts.Render("~/bundles/scripts")
    @Styles.Render("~/bundles/styles")
    <title></title>
</head>
<body>
    <div class="controls">
        <a class="control updatestats mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">Update</a>
    </div>
    <div id="mainContent">
        <div class="leagueSummary">
        </div>
    </div>
    <div id="hidden" style="visibility:hidden">
        <table class="owners mdl-data-table"></table>
        <div class="teamContent">
            <div class="teamTitle"></div>
            <div class="teamTable">
                <table class="team mdl-data-table"></table>
            </div>
        </div>
        <div class="demo-card-wide mdl-card mdl-shadow--2dp">
            <div class="mdl-card__title">
                <h2 class="mdl-card__title-text">Welcome</h2>
            </div>
            <div class="mdl-card__supporting-text">
                Lorem ipsum dolor sit amet, consectetur adipiscing elit.
                Mauris sagittis pellentesque lacus eleifend lacinia...
            </div>
            <div class="mdl-card__actions mdl-card--border">
                <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
                    Get Started
                </a>
            </div>
            <div class="mdl-card__menu">
                <button class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect">
                    <i class="material-icons">share</i>
                </button>
            </div>
        </div>
    </div>
</body>
</html>